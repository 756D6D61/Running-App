{
  "expo": {
    "name": "running app",
    "slug": "run",
    "privacy": "public",
    "sdkVersion": "34.0.0",
    "packagerOpts": {
      "sourceExts": [ "js", "json", "ts", "tsx", "jsx", "svg"],
      "config": "metro.config.js"
    },
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}